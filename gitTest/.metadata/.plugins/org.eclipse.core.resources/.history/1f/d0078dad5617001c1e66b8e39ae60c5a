package 자바페스티벌풀어보기;

public class ex3번홀수짝수100까지더하기 {

	public static void main(String[] args) {
		//1 -2 +3 -4 +5 -6 +7~-100
		int result = 0;
		for(int i=1;i<101;i++) {
			if(i/2==0) {
				i*=-1;
				result+=i;
			}else {
				result+=i;
			}
		}System.out.println("결과 : "+result);
		
		
	}

}
