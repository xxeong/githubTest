package 자바페스티벌1차;

public class ex22번별출력 {

	public static void main(String[] args) {
		// *
		// **
		// ***
		// ****
		// *****
		int n = 4;
		for (int j = 0; j < 5; j++) {
			for (int i = 0; i < n; i++) {
				System.out.print(" ");
			}
			n--;
			System.out.println();
			for(int i=0;i<n+1;i++)
				
					System.out.print("*");
			
		}

	}

}
