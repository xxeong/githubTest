package 자바페스티벌1차;

import java.util.Random;

public class ex18번중복없이로또뽑기 {

	public static void main(String[] args) {
		Random ran = new Random();
		int num = ran.nextInt(45)+1;
		for(int i = 0; i<7; i++) {
			System.out.println("행운의숫자 : "+num);
		}
		
		
		
		
		
		
		
		
	}

}
