package 자바페스티벌풀어보기;

import java.util.Scanner;

public class ex5번삼각형출력 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("행 개수 : ");
		int num = sc.nextInt();
		for (int i = 0; i < num; i++) {
			for (int j =0;j<i-1;j++) {
				System.out.print("*");
			}
		}
	}

}
