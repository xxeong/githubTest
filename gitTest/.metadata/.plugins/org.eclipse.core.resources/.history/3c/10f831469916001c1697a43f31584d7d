package 자바페스티벌1차;

public class ex22번별출력 {

	public static void main(String[] args) {
		// *
		// **
		// ***
		// ****
		// *****
		for(int i = 0; i<5 ;i++) {
		for(int j = 0; j<4 ;j++ ) {
			System.out.print(" ");
			for(int k = 0; k<j; k++) {
				System.out.print("*");
			}
		}
		}
		
		
		

	}

}
