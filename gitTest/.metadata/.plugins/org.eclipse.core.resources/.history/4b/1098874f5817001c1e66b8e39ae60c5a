package 자바페스티벌풀어보기;

public class ex3번홀수짝수100까지더하기 {

	public static void main(String[] args) {
		//1 -2 +3 -4 +5 -6 +7~-100
		int result = 0;
		int umsu= -1;
		
		for(int i=1;i<101;i++) {
			if(i%2==0) {
				i*=umsu;
				result=i;
			}else {
				result+=i;
			}
			
			System.out.print(result+" ");
			
		}
		
		
	}

}
