package 자바페스티벌풀어보기;

import java.util.Scanner;

public class ex4번잔돈내주기 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("총금액 입력 : ");
		int input = sc.nextInt();
		
		System.out.println("잔돈 : "+input+"원");
		System.out.println("10000원 : "+input/10000+"개");
		System.out.print("5000원 : "+input%10000/5000+"개");
		System.out.print("1000원 : "+input%10000%5000/1000+"개");
		System.out.print("500원 : "+input/10000+"개");
		System.out.print("100원 : "+input/10000+"개");
		
		
		
		
		
		
		
		
		
		
		
	}

}
