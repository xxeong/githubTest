package 자바페스티벌1차;

import java.util.Scanner;

public class ex19번인덱스만큼별출력 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] array= new int[5];
		for(int i = 0;i<5;i++) {
			System.out.print(i+"번째 별의 수 : ");
			int num = sc.nextInt();
			array[i]=num;
			
			}for(int j=0;j<array.length;j++) {
				System.out.print("*");
			
				
				
			
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
