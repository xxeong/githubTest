package 자바페스티벌1차;

import java.util.Scanner;

public class ex5번별찍기 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("행 개수 : ");
		int hang = sc.nextInt();
		for(int j=0;j<hang;j++) {
			for(int i =0;i<hang;i++) {
				System.out.print("*");
		}
			System.out.println();
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
