package 자바페스티벌1차;

public class ex23번2차원배열90도회전 {

	public static void main(String[] args) {
		int[][] array = new int[5][5];
		int num = 1;
		for(int i = 0; i<array.length;i++) {
			for(int j = 0; j<array.length; j++) {
				array[i][j] = num;
				num++;
			}
		}System.out.println(array.length);
//		array[0][0] = 1
//		array[0][1] = 2
//		array[0][2] = 3
//		array[0][3] = 4
//		array[0][4] = 5
//		
//		array[1][0] = 6
//		array[1][1] = 7
//		array[1][2] = 8
//		array[1][3] = 9
//		array[1][4] = 10
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
