package 자바페스티벌1차;

public class ex22번별출력 {

	public static void main(String[] args) {
		// *
		// **
		// ***
		// ****
		// *****
		int n = 4;
		int m = 1;
		for (int j = 0; j < 5; j++) {
			for (int i = 0; i < n; i++) {
				System.out.print(" ");
			}
			n--;
			for(int i = 0;i<0;i++) {
			System.out.print("*");
			}
			
			
		}

	}

}
