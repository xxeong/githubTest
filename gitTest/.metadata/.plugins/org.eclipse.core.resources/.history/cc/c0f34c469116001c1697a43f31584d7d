package ex0916업캐스팅;

public class SnackMain {

	public static void main(String[] args) {
		
		// 업캐스팅을 통해서 Snack 객체로
		// 각각의 과자 봉지를 뜯어주세요!
		MySnack s = new MySnack();
		Snack snack = new Snack();
		
		snack = new Banana();
		
		snack.open();
		
		s.open(snack);
		
		
		
		
		
	}

}
