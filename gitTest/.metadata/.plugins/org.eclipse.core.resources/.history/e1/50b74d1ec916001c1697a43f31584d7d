package 추상클래스예제;

public class TempEmployee {
	
	private String empno;//사번
	private String name;//이름
	private int pay;//연봉
	
	public TempEmployee(String empno, String name, int pay) {
		this.empno = empno;
		this.name = name;
		this.pay = pay;
	}
	
	public int getMoneyPay() {
		return pay/12;
	}
	public String print() {
		return empno+" : "+name+" : "+pay;
	}
	
	
	
	
	
	
}
