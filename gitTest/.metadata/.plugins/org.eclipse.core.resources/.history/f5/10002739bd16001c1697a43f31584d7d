package 추상클래스예제;

public class RegularEmployee {
	
	private String empno;
	private String name;
	private int pay;
	private int bonus;
	public RegularEmployee(String i, String name, int pay, int bonus) {
		this.empno = i;
		this.name = name;
		this.pay = pay;
		this.bonus = bonus;
	}
	
	public int getMoneyPay() {
		return (pay+bonus)/12;
	}
	public String print() {
		return (empno+" : "+name+" : "+pay);
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
