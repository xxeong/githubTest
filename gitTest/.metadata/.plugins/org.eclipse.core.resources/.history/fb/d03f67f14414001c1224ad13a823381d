package ex0913객체지향;

public class StudentMain {

	public static void main(String[] args) {
		
		Student student1= new Student("박원호", "20180001", 20, 50, 89,77);
		
		Student student2 = new Student("이명호", "20180002", 20, 90, 100, 100);
		
		Student student3 = new Student("압살라말라이쿰", "1416038", 30, 100, 100, 100);
	
		student1.show();
		System.out.println("=================");
		student2.show();
		
		student3.show();
		
		
		
		
	}

}
